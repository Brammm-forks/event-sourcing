{
    "name": "prooph/event-sourcing",
    "description": "PHP EventSourcing library",
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "http://getprooph.org/",
    "authors": [
        {
            "name": "Alexander Miertsch",
            "email": "contact@prooph.de",
            "homepage": "http://www.prooph.de"
        },
        {
            "name": "Sascha-Oliver Prolic",
            "email": "saschaprolic@googlemail.com"
        }
    ],
    "keywords": [
        "EventSourcing",
        "DDD",
        "prooph"
    ],
    "require": {
        "php": "^7.1",
        "prooph/common" : "dev-php_7_1 as 4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "phpunit/phpunit-mock-objects": "^3.3.1",
        "prooph/event-store" : "dev-php_7_1 as 7.0.0",
        "friendsofphp/php-cs-fixer": "^1.12.2",
        "satooshi/php-coveralls": "^1.0",
        "malukenho/docheader": "^0.1.4"
    },
    "autoload": {
        "psr-4": {
            "Prooph\\EventSourcing\\": "src/"
        }
    },
    "suggest" : {
        "prooph/event-store" : "^7.0 - Use prooph/event-store to persist recorded events in event streams"
    },
    "autoload-dev": {
        "psr-4": {
            "ProophTest\\EventSourcing\\": "tests/"
        }
    }
}
