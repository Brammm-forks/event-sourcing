{
    "name": "prooph/event-sourcing",
    "description": "PHP EventSourcing library",
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "http://getprooph.org/",
    "authors": [
        {
            "name": "Alexander Miertsch",
            "email": "contact@prooph.de",
            "homepage": "http://www.prooph.de"
        },
        {
            "name": "Sascha-Oliver Prolic",
            "email": "saschaprolic@googlemail.com"
        }
    ],
    "keywords": [
        "EventSourcing",
        "DDD",
        "prooph"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "prooph/common" : "4.0.x-dev",
        "prooph/event-store" : "7.0.x-dev"
    },
    "require-dev": {
        "container-interop/container-interop": "^1.1",
        "sandrokeil/interop-config": "^2.0.1",
        "phpunit/phpunit": "^5.7",
        "prooph/php-cs-fixer-config": "^0.1.1",
        "phpspec/prophecy": "dev-patch-1 as 1.6.2",
        "satooshi/php-coveralls": "^1.0",
        "prooph/bookdown-template": "^0.2.3",
        "malukenho/docheader": "^0.1.4"
    },
    "conflict": {
        "sandrokeil/interop-config": "<2.0.1"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "5.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Prooph\\EventSourcing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProophTest\\EventSourcing\\": "tests/",
            "ProophTest\\EventStore\\": "vendor/prooph/event-store/tests/"
        }
    },
    "config": {
        "preferred-install": {
            "prooph/*": "source"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "php-cs-fixer fix -v --diff --dry-run",
        "cs-fix": "php-cs-fixer fix -v --diff",
        "test": "phpunit"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/prolic/prophecy.git"
        }
    ]
}
